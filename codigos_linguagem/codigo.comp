const max = 3;

var quantidade : integer , altura, largura : real;

begin
	var area: real,
	cont : integer;
	
	read(altura, largura);

	area := (altura * largura);
	
	if (area+0) <= 100 then
	begin			
		for cont = 0 to max do 
		begin
			quantidade := area * cont;
			write(quantidade);
			cont := cont + 1;
		end;
	end

		else
		begin
			write(quantidade);
		end; {$$$@@s}
end.